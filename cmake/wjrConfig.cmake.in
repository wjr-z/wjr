# wjr CMake configuration file.

@PACKAGE_INIT@

list(APPEND CMAKE_PREFIX_PATH "${CMAKE_CURRENT_LIST_DIR}/../fmt")
list(APPEND CMAKE_PREFIX_PATH "${CMAKE_CURRENT_LIST_DIR}/../CpuFeatures")
list(APPEND CMAKE_PREFIX_PATH "${CMAKE_CURRENT_LIST_DIR}/../range-v3")

if (NOT TARGET fmt)
    find_package(fmt REQUIRED)
endif()

if (NOT TARGET CpuFeatures)
    find_package(CpuFeatures REQUIRED)
endif()

if (NOT TARGET range-v3)
    find_package(range-v3 REQUIRED)
endif()

include ("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
include ("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Alias.cmake")

add_library(wjr::wjr ALIAS wjr)